<tr>
  <td>
    <%= image_tag Settings.url_table %>
    <%= link_to table.number, table %>
  </td>
  <td><span><%= table.description %></span></td>
  <td><span><%= table.amount_chair %></span></td>
  <td>
    <%= link_to t(".delete"), table, method: :delete,
  data: { confirm: t(".sure") } %> | 
  <%= link_to t(".show"), table %> |
  <%= link_to t(".edit"), edit_table_path(table) %>
  </td>
</tr>
